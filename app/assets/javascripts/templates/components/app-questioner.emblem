each catalog in catalogs
  = catalog

unless modal
  div style="background-color: #ddd;"
    h5 :: debug ::

    p
      |Questions???
      strong= questions.length
      ul: each question in questions
        li= question.name
  
    p
      |Responses???
      strong= responses.length
      ul: each response in responses
        li {{response.name}} :: {{response.value}}
  
nav
  a.previous-link href="" click="previousSection" <<
  each section in sections
    |&nbsp;&nbsp;
    a href="" click="setSection section.number" class={::section-number-link section.selected:selected}= section.number
  |&nbsp;&nbsp;
  a.next-link href="" click="nextSection" >>

= partial "questioner/section"